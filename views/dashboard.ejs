            <p>Hygiene</p>
            <input type="range" min="1" max="9"  class="filter-slider">
            <div class="filter-labels-hygiene">
                <span>Clean</span> 
                <span>Decent</span> <span>Unimportant</span>
              </div>
           </div> 
      
          <div class="filter-atmosphere"> 
            <p>Atmosphere</p>
            <input type="range" min="1" max="9" value="5" class="filter-slider">
            <div class="filter-labels-atmo">
              <span>Dancing</span> <span>Chatting</span></div>
          </div> 
      
          <div class="filter-safety"> 
            <p>Safety</p>
            <input type="range" min="1" max="9" value="5" class="filter-slider">
           <div class="filter-labels-safety"> 
             <span>People feel safe</span> <span>Unimportant</span></div>
          </div> 

          
      
    
          <button class="apply-filter" >Apply filters</button>
      </form>



  </div>

